# API Server Configuration
TL_SERVER_BASE_PATH=/api
TL_SERVER_PORT=3005

# Database Configuration
DATABASE_URL=postgresql://agent_user:agent_password@localhost:5432/agent_timeline

# GUI Database Connection (if different from server)
VITE_DATABASE_URL=${DATABASE_URL}
VITE_API_BASE_URL=http://localhost:3005/api

# Development Configuration
NODE_ENV=development
PORT=3000

# Optional: Custom polling interval (milliseconds)
VITE_POLLING_INTERVAL=1500

# Optional: Custom retry intervals for error recovery
VITE_RETRY_INTERVALS=1000,2000,4000,8000,16000