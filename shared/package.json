{
  "name": "agent-timeline-shared",
  "version": "1.0.0",
  "description": "Shared types and constants for AI Agent Timeline MCP Server",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
