{
  "name": "agent-timeline-gui",
  "version": "1.0.0",
  "description": "React GUI for AI Agent Timeline",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:api": "node server.cjs",
    "dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "agent-timeline-shared": "workspace:*",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/pg": "^8.10.7",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^5.0.0",
    "typescript": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "concurrently": "^8.0.0"
  }
}